{
  "name": "orbit-type-generator",
  "version": "1.0.0-alpha.3",
  "description": "Create TypeScript definition file from a Orbit.js Schema definition",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exivity/react-orbitjs.git"
  },
  "keywords": [
    "react",
    "orbitjs",
    "typescript"
  ],
  "author": "Joram van den Boezem <joram@vandenboezem.nl>",
  "bugs": {
    "url": "https://github.com/exivity/react-orbitjs/issues"
  },
  "homepage": "https://github.com/exivity/react-orbitjs#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "/bin",
    "/lib"
  ],
  "bin": "./bin/run",
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint 'src/**/*.ts' --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "rm -rf ./lib && yarn build:static && tsc",
    "build:static": "copyfiles -u 1 src/__templates__/** lib/",
    "build:watch": "rm -rf ./lib && yarn build:static && tsc --watch",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "camelcase": "^5.3.1",
    "slash": "^3.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "@orbit/data": "^0.15.23",
    "@orbit/utils": "^0.15.23"
  },
  "devDependencies": {
    "@orbit/data": "^0.15.23",
    "@orbit/utils": "^0.15.23",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.8",
    "copyfiles": "^2.1.0",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1"
  }
}
